{
  "name": "express-boiler-plate",
  "version": "1.0.0",
  "description": "It provides a platform for easy creation of express server app",
  "main": "jest.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinefe/express-boiler-plate.git"
  },
  "author": "Justin EFe Igugu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justinefe/express-boiler-plate/issues"
  },
  "homepage": "https://github.com/justinefe/express-boiler-plate#readme",
  "scripts": {
    "start": "npm run build && node dist/index",
    "migrate_test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate_test:undo": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest": "npm run migrate_test:undo && npm run migrate_test",
    "test": "jest --detectOpenHandles --verbose --coverage --forceExit",
    "dev": "nodemon --exec babel-node server/index.js",
    "migrate": "cross-env sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "build": "cross-env NODE_ENV=production sequelize db:migrate && babel server --copy-files --out-dir dist",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^25.5.4",
    "nodemon": "^2.0.7",
    "supertest": "^4.0.2",
    "supertest-session": "^4.1.0"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.25.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "cryptr": "^6.0.2",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "easy-rbac": "^3.1.1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-form-data": "^2.0.14",
    "faker": "^5.4.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "os": "^0.1.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.5.1",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-slugify": "^0.9.1",
    "socket.io": "^2.4.1",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  }
}
